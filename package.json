{
  "name": "infinity-playblocks",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install": "cd client && npm install && cd ../server && npm install",
    "prepare": "husky install",
    "docker:up": "./docker-compose.sh up",
    "docker:up:dev": "./docker-compose.sh up --dev",
    "docker:build": "./docker-compose.sh up --build",
    "docker:build:dev": "./docker-compose.sh up --build --dev",
    "docker:down": "docker-compose down"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "husky": "^8.0.3"
  }
}
